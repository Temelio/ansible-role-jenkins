<?xml version='1.0' encoding='UTF-8'?>
<org.jenkinsci.plugins.graphiteIntegrator.DescriptorImpl plugin="graphiteIntegrator@{{ jenkins_plugin.version }}">
{% if jenkins_plugin_cfg_graphite_integrator_metrics_map %}
  <metricsMap class="hudson.util.CopyOnWriteMap$Hash">
{% for jenkins_metric_map in jenkins_plugin_cfg_graphite_integrator_metrics_map %}
{% endfor %}
  </metricsMap>
{% else %}
  <metricsMap class="hudson.util.CopyOnWriteMap$Hash"/>
{% endif %}
{% if jenkins_plugin_cfg_graphite_integrator_servers %}
  <servers>
{% for jenkins_graphite_server in jenkins_plugin_cfg_graphite_integrator_servers %}
{% endfor %}
  </servers>
{% else %}
  <servers/>
{% endif %}
  <validator>
    <ipPatern>(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)</ipPatern>
    <portPatern>([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])</portPatern>
  </validator>
  <baseQueueName>{{ jenkins_plugin_cfg_graphite_integrator_base_queue_name }}</baseQueueName>
</org.jenkinsci.plugins.graphiteIntegrator.DescriptorImpl>